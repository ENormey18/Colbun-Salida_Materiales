<core:FragmentDefinition
    xmlns="sap.m"
    xmlns:core="sap.ui.core"
>
    <SelectDialog
        id="receptorSelectDialog"
        title="Seleccionar Receptor"
        contentHeight="50%" 
        items="{
            path: 'salidaMateriales>/Receptores'
        }"
        search="onReceptoresValueHelpSearch"
        confirm="onReceptoresValueHelpClose"
        cancel="onReceptoresValueHelpClose"
    >
        <StandardListItem
            title="{salidaMateriales>Nombre}"
            description="{salidaMateriales>Usuario}"
            type="Active"
        />
    </SelectDialog>
</core:FragmentDefinition>