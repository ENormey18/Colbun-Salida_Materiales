<mvc:View
    controllerName="salidademateriales.controller.Listado"
    xmlns="sap.m"
    xmlns:f="sap.f"
    xmlns:mvc="sap.ui.core.mvc"
    xmlns:core="sap.ui.core"
>
    <f:DynamicPage
        id="page"
        toggleHeaderOnTitleClick="true"
    >
        <f:title>
            <f:DynamicPageTitle>
                <f:heading>
                    <Title text="Listado" level="H3"/>
                </f:heading>
            </f:DynamicPageTitle>
        </f:title>
        <f:header>
            <f:DynamicPageHeader>
                <core:Fragment
                    fragmentName="salidademateriales.view.fragments.listado.FilterBar"
                    type="XML"
                />
            </f:DynamicPageHeader>
        </f:header>
        <f:content>
            <core:Fragment
                fragmentName="salidademateriales.view.fragments.listado.ReservasTable"
                type="XML"
            />
        </f:content>
    </f:DynamicPage>
</mvc:View>
